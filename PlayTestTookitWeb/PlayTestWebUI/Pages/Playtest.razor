 @page "/playtest"

<h3>@PlaytestName</h3>

@if (!HasData)
{
    <p><em>Loading...</em></p>
}
else
{
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>StartTime</th>
          <th>Input</th>
        </tr>
      </thead>
      <tbody>
    @foreach(var project in Data)
    {
        <tr>
          <td>@project.Id</td>
          <td>@GetDate(project.StartTime)</td>

          @foreach(var input in project.Input)    
          {
            <td>@input</td>
          }

        </tr>
    }
      </tbody>
    </table>
}